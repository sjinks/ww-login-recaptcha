{
	"name" : "wildwolf/ww-login-recaptcha",
	"type" : "wordpress-plugin",
	"license" : "MIT",
	"authors" : [{
			"name" : "Volodymyr Kolesnykov",
			"email" : "volodymyr@wildwolf.name",
			"homepage" : "https://wildwolf.name/",
			"role" : "Developer"
		}
	],
	"description" : "WordPress Plugin",
	"require" : {
		"composer/installers" : "^2",
