		"wildwolf/ww-hide-wplogin" : "WordPress plugin to hide wp-login.php and use a custom URL for logging in"
	},
	"autoload" : {
		"psr-4" : {
			"WildWolf\\LoginRecaptcha\\" : "inc"
		}
	},
	"keywords" : [
		"plugin",
		"security",
		"authentication",
		"CAPTCHA",
		"reCAPTCHA",
		"wordpress"
	],
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
